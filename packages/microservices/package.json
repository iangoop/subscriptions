{
  "name": "@subscriptions/microservices",
  "version": "1.0.0",
  "description": "",
  "main": "lib/src/index.js",
  "type": "commonjs",
  "engines": {
    "node": "20"
  },
  "scripts": {
    "build": "tsc && tsc-alias",
    "build:watch": "concurrently \"tsc -w\" \"tsc-alias -w\"",
    "dev": "dotenvx run -f env/development.env -f ../../env/development.env -- nodemon",
    "start": "node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "npx eslint --ext .ts src/",
    "lint:tests": "npx eslint --ext .ts spec/",
    "prettier-format": "prettier --write \"{src/**/*.,*}{ts,tsx,json}\""
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "ignore": [
      "src/public"
    ],
    "exec": "../../node_modules/.bin/ts-node --files -r tsconfig-paths/register ./src"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^5.6.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/sensible": "^5.1.1",
    "@fastify/type-provider-typebox": "^2.4.0",
    "@sinclair/typebox": "^0.25.11",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "command-line-args": "^5.2.1",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "fastify": "^4.10.2",
    "fastify-plugin": "^4.3.0",
    "firebase": "^9.14.0",
    "firebase-admin": "^12.6.0",
    "firebase-functions": "^6.0.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.2.0",
    "@types/pino": "^7.0.5",
    "@types/uuid": "^9.0.0",
    "express": "^4.21.1"
  }
}
